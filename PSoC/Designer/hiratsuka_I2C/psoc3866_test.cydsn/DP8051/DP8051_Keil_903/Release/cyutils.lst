                      uint32 CyGetReg24(uint32 volatile * addr)
                      {
                          uint8 volatile *tmpAddr;
                          uint32 value;
              
                          tmpAddr = (uint8 volatile *) addr;
              
                          value  =  (uint32) tmpAddr[0u];
                          value |= ((uint32) tmpAddr[1u] << 8u );
                          value |= ((uint32) tmpAddr[2u] << 16u);
              
